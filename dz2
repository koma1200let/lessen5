money=input("назовите номинал ")
money=int(money)
if money==1:
    print(f"изображённый портрет принадлежит Джорджу Вашингтону")
elif money==2:
    print(f"изображённый портрет принадлежит Томасу Джефферсону")
elif money==5:
    print(f"изображённый портрет принадлежит Аврааму Линкольну")
elif money==10:
    print(f"изображённый портрет принадлежит Александр Гамильтон")
elif money==20:
    print(f"изображённый портрет принадлежит Эндрю Джексону")
elif money==50:
    print(f"изображённый портрет принадлежит Улиссу Гранту")
elif money==100:
    print(f"изображённый портрет принадлежит Бенджамину Франклину")
else:
    print(f"такой купюры не существует")
