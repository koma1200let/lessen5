dts=input("введите громкость ")
dts=int(dts)
if dts==40:
    print(f"это тихая комната")
elif dts==106:
    print(f"это газовая газонокосилка")
elif dts==70:
    print(f"это будильник")
elif dts==130:
    print(f"это отбойный молоток")
elif dts>=41:
    print(f"это между будильником и тихой комнатой")
elif dts>=71:
    print(f"это между будильником и газовой газонокосилкой")
elif dts>=107:
    print(f"это между газовой газонокосилкой и отбойным молотком")
elif dts>=131:
    print(f"это громче отбойного молотка")
elif dts<=39:
    print(f"это тише чем в тихой комнате")
